{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier/react",
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "globals": {
    "widndow": true,
    "document": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./src"],
        "extensions": [".js"]
      }
    }
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-console": 0,
    "react/jsx-props-no-spreading": 0
  }
}
